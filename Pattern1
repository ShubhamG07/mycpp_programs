// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;
    int i=1;
    while(i<=n){
        int k=1;
        while(k<=i-1){
            cout<<n-i+k;
            k++;
        }
        int j=1;
        while(j<=n-i+1){
            cout<<n;
            j++;
        }
    
        
        cout<<endl;
        i++;
    }
}
    
